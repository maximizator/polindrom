import telebot
import time
from telebot import types
from telegram import ReplyKeyboardMarkup
import requests
from bs4 import *

def get_html(url):
    request = requests.get(url)
    return False if request.status_code == 404 else request.text



TOKEN = '6251292819:AAFFFrU1ds9JHIO3I_ZUMTWYWl6qSS7_p1M'
bot = telebot.TeleBot(TOKEN)


@bot.message_handler(commands=['start'])
def start(message):
    # –°–æ–∑–¥–∞–µ–º –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É —Å –≤—ã–±–æ—Ä–æ–º —Å—Ç—Ä–∞–Ω—ã –ø–æ—Å–æ–ª—å—Å—Ç–≤–∞
    keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
    button_usa = types.KeyboardButton(text="–°–®–ê")
    button_uk = types.KeyboardButton(text="–§—Ä–∞–Ω—Ü–∏—è")
    button_fr = types.KeyboardButton(text="–ö–∏—Ç–∞–π")
    button_ch = types.KeyboardButton(text="–ì–µ—Ä–º–∞–Ω–∏—è")
    button_it = types.KeyboardButton(text="–ò—Ç–∞–ª–∏—è")
    keyboard.add(button_usa, button_uk, button_fr, button_ch, button_it)

    # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ –∏ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É
    bot.send_message(message.chat.id, "–ü—Ä–∏–≤–µ—Ç! –Ø –±–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑. –ß—Ç–æ–±—ã –Ω–∞—á–∞—Ç—å —Ä–∞–±–æ—Ç—É, –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤—ã–±–µ—Ä–∏—Ç–µ "
                                      "—Å—Ç—Ä–∞–Ω—É –ø–æ—Å–æ–ª—å—Å—Ç–≤–∞:", reply_markup=keyboard)


@bot.message_handler(func=lambda message: True)
def handle_message(message):
    # –û–±—Ä–∞–±–æ—Ç–∫–∞ –≤—ã–±–æ—Ä–∞ —Å—Ç—Ä–∞–Ω—ã –ø–æ—Å–æ–ª—å—Å—Ç–≤–∞
    if message.text == "–§—Ä–∞–Ω—Ü–∏—è":
        bot.send_message(message.chat.id, "üåü –•–æ—á–µ—à—å –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –≤–∏–∑—É –§—Ä–∞–Ω—Ü–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è "
                                          "–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑"
                                          "–ø–æ–º–æ–∂–µ—Ç\n\nüåü –£–∂–µ –∑–∞–ø–∏—Å–∞–ª—Å—è, –Ω–æ —Ö–æ—á–µ—à—å –ø–µ—Ä–µ–Ω–µ—Å—Ç–∏ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –¥—Ä—É–≥—É—é "
                                          "–¥–∞—Ç—É?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –í—Å—ë –µ—â—ë –Ω–µ –∑–Ω–∞–µ—à—å, "
                                          "–≤ –∫–∞–∫–æ–π —Å—Ç—Ä–∞–Ω–µ"
                                          "–ª—É—á—à–∞—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –≤—ã–¥–∞—á–µ –≤–∏–∑ –§—Ä–∞–Ω—Ü–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ "
                                          "—Ä–∞—Å—Å–∫–∞–∂–µ—Ç\n\nüåü –ù–µ—Ç –∑–∞—Ä—É–±–µ–∂–Ω–æ–π –∫–∞—Ä—Ç—ã, —á—Ç–æ–±—ã –æ–ø–ª–∞—Ç–∏—Ç—å –∫–æ–Ω—Å—É–ª—å—Å–∫–∏–π —Å–±–æ—Ä –¥–ª—è "
                                          "–§—Ä–∞–Ω—Ü–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –ù–µ —É–≤–µ—Ä–µ–Ω, —á—Ç–æ –∑–Ω–∞–µ—à—å –≤—Å–µ "
                                          "–Ω—é–∞–Ω—Å—ã –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è –∞–Ω–∫–µ—Ç—ã DS-160 –∏ –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏—è —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—è –¥–ª—è "
                                          "–§—Ä–∞–Ω—Ü–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç")

        keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button_booking = types.KeyboardButton(text="–ó–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_reschedule = types.KeyboardButton(text="–ü–µ—Ä–µ–∑–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_legal = types.KeyboardButton(text="–Æ—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –¥–∞–Ω–Ω—ã–µ")
        button_consult = types.KeyboardButton(text="–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ")

        keyboard.add(button_booking, button_reschedule, button_legal, button_consult)
        time.sleep(0.5)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:", reply_markup=keyboard,
                         reply_to_message_id=message.message_id)

    elif message.text == "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ":
        # –ü–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –Ω–∞ —Å–ª–µ–¥—É—é—â–µ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
        bot.send_message(message.chat.id, "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è —Å –≤–∏–∑–æ–≤—ã–º\n—Å–ø–µ—Ü–∏–∞–ª–∏—Å—Ç–æ–ºü¶∏‚Äç\n‚è±–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å - 1 —á–∞—Å\nüí≤–°—Ç–æ–∏–º–æ—Å—Ç—å"
                                          " - 20 000‚ÇΩ\n \n –í –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—é –≤—Ö–æ–¥–∏—Ç‚¨áÔ∏è\n \nüîé –û—Ç–≤–µ—Ç –Ω–∞ –ª—é–±–æ–π –∏–∑ "
                                          "–≤–∞—à–∏—Ö –≤–æ–ø—Ä–æ—Å–æ–≤\n–∫–∞—Å–∞—Ç–µ–ª—å–Ω–æ –≤–∏–∑—ã –§—Ä–∞–Ω—Ü–∏–∏\nüìù –ü—Ä–æ—Ä–∞–±–æ—Ç–∫–∞ –≤–∞—à–µ–≥–æ –∫–µ–π—Å–∞, –Ω–∞ —Å–∫–æ–ª—å–∫–æ\n"
                                          "—ç—Ç–æ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –≤–æ–∑–º–æ–∂–Ω—ã–º –ø—Ä–∏\n–ø–µ—Ä–≤–∏—á–Ω–æ–π –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–∏\n"
                                          "üíå –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ —Å\n–Ω–∞—à–µ–π —Å—Ç–æ—Ä–æ–Ω—ã –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–∞–∏–ª—É—á—à–µ–≥–æ\n"
                                          "—Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞ –ø–æ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—é")


    elif message.text == "–°–®–ê":
        bot.send_message(message.chat.id, "üåü –•–æ—á–µ—à—å –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –≤–∏–∑—É –°–®–ê?\nüöÄ –ë–æ—Ç –¥–ª—è "
                                          "–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑"
                                          "–ø–æ–º–æ–∂–µ—Ç\n\nüåü –£–∂–µ –∑–∞–ø–∏—Å–∞–ª—Å—è, –Ω–æ —Ö–æ—á–µ—à—å –ø–µ—Ä–µ–Ω–µ—Å—Ç–∏ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –¥—Ä—É–≥—É—é "
                                          "–¥–∞—Ç—É?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –í—Å—ë –µ—â—ë –Ω–µ –∑–Ω–∞–µ—à—å, "
                                          "–≤ –∫–∞–∫–æ–π —Å—Ç—Ä–∞–Ω–µ"
                                          "–ª—É—á—à–∞—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –≤—ã–¥–∞—á–µ –≤–∏–∑ –°–®–ê?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ "
                                          "—Ä–∞—Å—Å–∫–∞–∂–µ—Ç\n\nüåü –ù–µ—Ç –∑–∞—Ä—É–±–µ–∂–Ω–æ–π –∫–∞—Ä—Ç—ã, —á—Ç–æ–±—ã –æ–ø–ª–∞—Ç–∏—Ç—å –∫–æ–Ω—Å—É–ª—å—Å–∫–∏–π —Å–±–æ—Ä –¥–ª—è "
                                          "–°–®–ê?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –ù–µ —É–≤–µ—Ä–µ–Ω, —á—Ç–æ –∑–Ω–∞–µ—à—å –≤—Å–µ "
                                          "–Ω—é–∞–Ω—Å—ã –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è –∞–Ω–∫–µ—Ç—ã DS-160 –∏ –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏—è —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—è –¥–ª—è "
                                          "–°–®–ê?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç")

        keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button_booking = types.KeyboardButton(text="–ó–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_reschedule = types.KeyboardButton(text="–ü–µ—Ä–µ–∑–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_legal = types.KeyboardButton(text="–Æ—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –¥–∞–Ω–Ω—ã–µ")
        button_consult = types.KeyboardButton(text="–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ")
        reply_markup = ReplyKeyboardMarkup([[button_consult]], one_time_keyboard=True)

        keyboard.add(button_booking, button_reschedule, button_legal, button_consult)
        time.sleep(0.5)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:", reply_markup=keyboard,
                         reply_to_message_id=message.message_id)

    def button_consult_handler(update, context):
        message = "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è —Å –≤–∏–∑–æ–≤—ã–º\n—Å–ø–µ—Ü–∏–∞–ª–∏—Å—Ç–æ–ºü¶∏‚Äç\n‚è±–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å - 1 —á–∞—Å\nüí≤–°—Ç–æ–∏–º–æ—Å—Ç—å"
        ' - 20 000‚ÇΩ\n \n –í –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—é –≤—Ö–æ–¥–∏—Ç‚¨áÔ∏è\n \nüîé –û—Ç–≤–µ—Ç –Ω–∞ –ª—é–±–æ–π –∏–∑ '
        "–≤–∞—à–∏—Ö –≤–æ–ø—Ä–æ—Å–æ–≤\n–∫–∞—Å–∞—Ç–µ–ª—å–Ω–æ –≤–∏–∑—ã –°–®–ê\nüìù –ü—Ä–æ—Ä–∞–±–æ—Ç–∫–∞ –≤–∞—à–µ–≥–æ –∫–µ–π—Å–∞, –Ω–∞ —Å–∫–æ–ª—å–∫–æ\n"
        "—ç—Ç–æ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –≤–æ–∑–º–æ–∂–Ω—ã–º –ø—Ä–∏\n–ø–µ—Ä–≤–∏—á–Ω–æ–π –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–∏\n"
        "üíå –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ —Å\n–Ω–∞—à–µ–π —Å—Ç–æ—Ä–æ–Ω—ã –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–∞–∏–ª—É—á—à–µ–≥–æ\n"
        "—Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞ –ø–æ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—é"
        update.message.reply_text(message)


    elif message.text == "–ö–∏—Ç–∞–π":
        bot.send_message(message.chat.id, "üåü –•–æ—á–µ—à—å –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –≤–∏–∑—É –ö–∏—Ç–∞–π?\nüöÄ –ë–æ—Ç –¥–ª—è "
                                          "–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑"
                                          "–ø–æ–º–æ–∂–µ—Ç\n\nüåü –£–∂–µ –∑–∞–ø–∏—Å–∞–ª—Å—è, –Ω–æ —Ö–æ—á–µ—à—å –ø–µ—Ä–µ–Ω–µ—Å—Ç–∏ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –¥—Ä—É–≥—É—é "
                                          "–¥–∞—Ç—É?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –í—Å—ë –µ—â—ë –Ω–µ –∑–Ω–∞–µ—à—å, "
                                          "–≤ –∫–∞–∫–æ–π —Å—Ç—Ä–∞–Ω–µ"
                                          "–ª—É—á—à–∞—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –≤—ã–¥–∞—á–µ –≤–∏–∑ –ö–∏—Ç–∞—è?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ "
                                          "—Ä–∞—Å—Å–∫–∞–∂–µ—Ç\n\nüåü –ù–µ—Ç –∑–∞—Ä—É–±–µ–∂–Ω–æ–π –∫–∞—Ä—Ç—ã, —á—Ç–æ–±—ã –æ–ø–ª–∞—Ç–∏—Ç—å –∫–æ–Ω—Å—É–ª—å—Å–∫–∏–π —Å–±–æ—Ä –¥–ª—è "
                                          "–ö–∏—Ç–∞—è?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –ù–µ —É–≤–µ—Ä–µ–Ω, —á—Ç–æ –∑–Ω–∞–µ—à—å –≤—Å–µ "
                                          "–Ω—é–∞–Ω—Å—ã –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è –∞–Ω–∫–µ—Ç—ã DS-160 –∏ –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏—è —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—è –¥–ª—è "
                                          "–ö–∏—Ç–∞—è?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç")

        keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button_booking = types.KeyboardButton(text="–ó–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_reschedule = types.KeyboardButton(text="–ü–µ—Ä–µ–∑–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_legal = types.KeyboardButton(text="–Æ—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –¥–∞–Ω–Ω—ã–µ")
        button_consult = types.KeyboardButton(text="–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ")

        keyboard.add(button_booking, button_reschedule, button_legal, button_consult)
        time.sleep(0.5)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:", reply_markup=keyboard,
                         reply_to_message_id=message.message_id)

    elif message.text == "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ":
        # –ü–µ—Ä–µ–Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –Ω–∞ —Å–ª–µ–¥—É—é—â–µ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
        bot.send_message(message.chat.id, "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è —Å –≤–∏–∑–æ–≤—ã–º\n—Å–ø–µ—Ü–∏–∞–ª–∏—Å—Ç–æ–ºü¶∏‚Äç\n‚è±–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å - 1 —á–∞—Å\nüí≤–°—Ç–æ–∏–º–æ—Å—Ç—å"
                                          " - 20 000‚ÇΩ\n \n –í –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—é –≤—Ö–æ–¥–∏—Ç‚¨áÔ∏è\n \nüîé –û—Ç–≤–µ—Ç –Ω–∞ –ª—é–±–æ–π –∏–∑ "
                                          "–≤–∞—à–∏—Ö –≤–æ–ø—Ä–æ—Å–æ–≤\n–∫–∞—Å–∞—Ç–µ–ª—å–Ω–æ –≤–∏–∑—ã –ö–∏—Ç–∞—è\nüìù –ü—Ä–æ—Ä–∞–±–æ—Ç–∫–∞ –≤–∞—à–µ–≥–æ –∫–µ–π—Å–∞, –Ω–∞ —Å–∫–æ–ª—å–∫–æ\n"
                                          "—ç—Ç–æ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç—Å—è –≤–æ–∑–º–æ–∂–Ω—ã–º –ø—Ä–∏\n–ø–µ—Ä–≤–∏—á–Ω–æ–π –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏–∏\n"
                                          "üíå –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ —Å\n–Ω–∞—à–µ–π —Å—Ç–æ—Ä–æ–Ω—ã –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–∞–∏–ª—É—á—à–µ–≥–æ\n"
                                          "—Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞ –ø–æ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—é")

    elif message.text == "–ì–µ—Ä–º–∞–Ω–∏—è":
        bot.send_message(message.chat.id, "üåü –•–æ—á–µ—à—å –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –≤–∏–∑—É –ì–µ—Ä–º–∞–Ω–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è "
                                          "–±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑"
                                          "–ø–æ–º–æ–∂–µ—Ç\n\nüåü –£–∂–µ –∑–∞–ø–∏—Å–∞–ª—Å—è, –Ω–æ —Ö–æ—á–µ—à—å –ø–µ—Ä–µ–Ω–µ—Å—Ç–∏ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ –Ω–∞ –¥—Ä—É–≥—É—é "
                                          "–¥–∞—Ç—É?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –í—Å—ë –µ—â—ë –Ω–µ –∑–Ω–∞–µ—à—å, "
                                          "–≤ –∫–∞–∫–æ–π —Å—Ç—Ä–∞–Ω–µ"
                                          "–ª—É—á—à–∞—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –≤—ã–¥–∞—á–µ –≤–∏–∑ –ì–µ—Ä–º–∞–Ω–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ "
                                          "—Ä–∞—Å—Å–∫–∞–∂–µ—Ç\n\nüåü –ù–µ—Ç –∑–∞—Ä—É–±–µ–∂–Ω–æ–π –∫–∞—Ä—Ç—ã, —á—Ç–æ–±—ã –æ–ø–ª–∞—Ç–∏—Ç—å –∫–æ–Ω—Å—É–ª—å—Å–∫–∏–π —Å–±–æ—Ä –¥–ª—è "
                                          "–ì–µ—Ä–º–∞–Ω–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç\n\nüåü –ù–µ —É–≤–µ—Ä–µ–Ω, —á—Ç–æ –∑–Ω–∞–µ—à—å "
                                          "–≤—Å–µ"
                                          "–Ω—é–∞–Ω—Å—ã –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è –∞–Ω–∫–µ—Ç—ã DS-160 –∏ –ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏—è —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏—è –¥–ª—è "
                                          "–ì–µ—Ä–º–∞–Ω–∏–∏?\nüöÄ –ë–æ—Ç –¥–ª—è –±—Ä–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏—è –≤–∏–∑ –ø–æ–º–æ–∂–µ—Ç")

        keyboard = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button_booking = types.KeyboardButton(text="–ó–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_reschedule = types.KeyboardButton(text="–ü–µ—Ä–µ–∑–∞–ø–∏—Å—å –Ω–∞ —Å–æ–±–µ—Å–µ–¥–æ–≤–∞–Ω–∏–µ")
        button_legal = types.KeyboardButton(text="–Æ—Ä–∏–¥–∏—á–µ—Å–∫–∏–µ –¥–∞–Ω–Ω—ã–µ")
        button_consult = types.KeyboardButton(text="–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ")

        keyboard.add(button_booking, button_reschedule, button_legal, button_consult)
        time.sleep(0.5)
        bot.send_message(message.chat.id, "–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:", reply_markup=keyboard,
                         reply_to_message_id=message.message_id)

    elif message.text == "–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü–∏—è –ø–æ –≤–∏–∑–µ":
